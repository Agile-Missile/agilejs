    "autoprefixer": "^10.4.21",
    "http-status-codes": "^2.3.0",
    "next": "^16.0.1",
    "next-auth": "^4.24.13",
    "next-sitemap": "^4.2.3",
    "nextra": "^4.6.0",
    "nextra-theme-docs": "^4.6.0",
    "postcss": "^8.5.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.16"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.16",
    "@types/animejs": "^3",
    "@types/node": "24.9.2",
